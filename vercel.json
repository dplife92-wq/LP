{
  "rewrites": [
    {
      "source": "/formation",
      "destination": "/"
    },
    {
      "source": "/formation.html",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/sections/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}